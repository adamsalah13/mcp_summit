{
  "server": {
    "name": "Summit Open Source MCP Processor",
    "version": "1.0.0",
    "output_dir": "summit_data",
    "max_file_size_mb": 50
  },
  "summit_api": {
    "base_url": "https://summit.sfu.ca",
    "timeout": 30,
    "max_concurrent": 3
  },
  "openai": {
